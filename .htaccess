# START MultiPHP Version
<FilesMatch "\.(php4|php5|php3|php2|php|phtml)$">
    SetHandler application/x-httpd-alt-php74___lsphp
</FilesMatch>
# END MultiPHP Version
# Nyalakan fitur RewriteEngine
RewriteEngine On

# Pastikan file atau folder yang diminta benar-benar ada
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Rewrite URL dari /split/{url_id} ke view_split_bill.php
RewriteRule ^([a-zA-Z0-9]+)/?$ view_split_bill.php?url_id=$1 [L,QSA]

# php -- BEGIN cPanel-generated handler, do not edit
# Set the “ea-php74” package as the default “PHP” programming language.
<IfModule mime_module>
  AddHandler application/x-httpd-ea-php74___lsphp .php .php7 .phtml
</IfModule>
# php -- END cPanel-generated handler, do not edit
